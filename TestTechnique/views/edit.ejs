<ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/ArrivalList/list">Liste d'Arrivées</a></li>
    <li class="breadcrumb-item active"><a>MODIFIER</a></li>
</ol>
<h1 class="display-4">Modifier une arrivée</h1>
<form method="POST" action="/ArrivalList/update/<%= arrivals.id %>">
    <div class="form-group">
        <label for="title">Titre</label>
        <input type="text" name="title" class="form-control" value="<%= arrivals.title %>">
    </div>
    <div class="form-group">
        <label for="name">name</label>
        <input type="text" name="name" class="form-control" value="<%= arrivals.name %>">
    </div>
    <div class="form-group">
        <label for="date">date</label>
        <input type="date" name="date" class="form-control" value="<%= arrivals.date %>">
    </div>
    <div class="form-group">
        <label for="time">time</label>
        <input type="time" name="time" class="form-control" value="<%= arrivals.time %>">
    </div>
    <div class="form-group">
        <label for="email">email</label>
        <input type="email" name="email" class="form-control" value="<%= arrivals.email %>">
    </div>
    <input type="submit" value="Modifier" class="btn btn-primary">
</form>